<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="LoginModel" type="System.Configuration.NameValueSectionHandler" />
  </configSections>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.1"/>
    </startup>
  <LoginModel>
    <add key="username" value="anonymous" />
    <add key="status" value="active" />
    <add key="pwd" value="pass" />
  </LoginModel>
  <connectionStrings>
    <add name="defaultConnection" connectionString="Data Source =|DataDirectory|\bakeryPR.db"/>
  </connectionStrings>
  
</configuration>
